[
	{
		"name": "temperature_sensor",
		"control_loop_module": "temperature_sensor.control_loop",
		"control_loop_sleep_ms": 5000,
		"registers": {
			"temperature": 0.0,
			"status": "ON"
		},
		"publishers": [
			{
				"id": "temperature_pub",
				"cycle_time_ms": 1000,
				"read": "temperature",
				"topic": "unit/temperature"
			}
		],
		"subscribers": [
			{
				"id": "temperature_sensor_command_sub",
				"write": "status",
				"topic": "unit/temperature/status"
			}
		]
	}
]
